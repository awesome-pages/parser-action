{
  "name": "@awesome-pages/parser-action",
  "version": "1.0.0",
  "description": "GitHub Action for parsing awesome-style README.md files into structured artifacts",
  "main": "dist/index.cjs",
  "type": "commonjs",
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "github-action",
    "awesome-list",
    "parser",
    "markdown"
  ],
  "author": "Awesome Pages",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@awesome-pages/parser": "^1.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
